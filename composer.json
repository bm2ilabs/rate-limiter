{
    "name": "bm2ilabs/rate-limiter",
    "description": "Multi-period rate limiting for Laravel applications with minute, hour, and day limits",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Amineware\\RateLimiter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amineware\\RateLimiter\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Boukraa Mohamed",
            "email": "amineware.me@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amineware\\RateLimiter\\RateLimiterServiceProvider"
            ],
            "aliases": {
                "MultiPeriodRateLimiter": "Amineware\\RateLimiter\\Facades\\MultiPeriodRateLimiter"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
